# Database
DATABASE_URL=postgresql://wob_user:wob_password@localhost:5432/wob_db
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=wob_user
DATABASE_PASSWORD=wob_password
DATABASE_NAME=wob_db

# Redis (for queue and cache)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# Application
PORT=3001
NODE_ENV=development

# Scraping Configuration
SCRAPE_DELAY_MS=2000
SCRAPE_MAX_RETRIES=3
CACHE_TTL_SECONDS=3600

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=100

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=debug
